                            android:textSize="@dimen/_8sdp" />
                    </RelativeLayout>

                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_13sdp"
                            android:text="Average"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_8sdp" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:gravity="center"
                            android:text="More"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_8sdp" />
                    </RelativeLayout>
                    <RelativeLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:layout_weight="1">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentStart="true"
                            android:layout_marginStart="@dimen/_13sdp"
                            android:text="Very Much"
                            android:textColor="@color/black"
                            android:textSize="@dimen/_7sdp" />
                    </RelativeLayout>


                </LinearLayout>

            </LinearLayout>
        </LinearLayout>


        <LinearLayout
            android:layout_marginBottom="@dimen/_30sdp"
            android:padding="@dimen/_15sdp"
            android:orientation="horizontal"
            android:background="@drawable/card_text_bg"
            android:layout_marginTop="@dimen/_30sdp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <ImageView

                android:layout_marginEnd="@dimen/_10sdp"
                android:layout_gravity="center_vertical"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/result_card_icon" />
            <TextView
                android:id="@+id/final_text"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="start"

                android:text="My annual Co2 emission is (2.6 T), 5 mangroves trees can sequester Co2 in 20 years"

                android:textSize="@dimen/_12sdp" />
        </LinearLayout>




        <LinearLayout

            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="2">

            <TextView
                android:id="@+id/cancl_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_30sdp"
                android:layout_weight="1"
                android:background="@drawable/button_empty"
                android:fontFamily="@font/font_med"
                android:gravity="center"
                android:paddingTop="@dimen/_5sdp"
                android:paddingBottom="@dimen/_3sdp"
                android:text="Cancel"
                android:textColor="@color/green_color"
                android:textSize="@dimen/_16sdp" />

            <TextView
                android:id="@+id/save_btn"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:background="@drawable/button_filled"
                android:fontFamily="@font/font_med"
                android:gravity="center"
                android:paddingTop="@dimen/_5sdp"
                android:paddingBottom="@dimen/_3sdp"
                android:text="Offset"
                android:textColor="@color/white"
                android:textSize="@dimen/_16sdp" />
        </LinearLayout>
    </LinearLayout>
</ScrollView>



